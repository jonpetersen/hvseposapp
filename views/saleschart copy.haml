%br
%p Current Month Sales
= line_chart [{name: "Sales Values", data:@sales_values},{name: "Sales Counts", data: @sales_counts}], height: "250px"
%p All Sales by Month
= column_chart @monthly_sales_values_for_chart, height: "250px"
%p All Sales by Day
= line_chart @all_sales_values_for_chart, height: "250px"
%p All Time VAT %'s
= line_chart @vatpercent_hash, height: "250px", colors: ["pink", "#999"]



	      